<%= form_with model: city do |f| %>

    <div>
        <%= f.label :name %>
        <%= f.text_field :name %>
    </div>

    <div>
        <%= f.label :state %>
        <%= f.select(:state_id, State.all.collect {|s| [s.name, s.id]}, {:prompt => 'Select State'}) %>
    </div>

    <div>
        <%= f.submit %>
    </div>
    
<% end %>